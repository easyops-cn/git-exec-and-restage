{
  "plugins": [
    ["transform-class-properties", { "spec": true }]
  ],
  "presets": [
    [
      "env",
      {
        "useBuiltIns": true,
        "targets": {
          "node": "6.9"
        }
      }
    ]
  ],
  "sourceMaps": "inline",
  "env": {
    "production": {
      "sourceMaps": false
    }
  }
}
